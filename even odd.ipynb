    "# Program to check if a number is even or odd\n",
    "# Input from the user\n",
    "\n",
    "\n",
    "num = int(input(\"enter any number to test whether it is odd or even:\"))\n",
    "\n",
    "if (num % 2) == 0:\n",
    "    print(\"the number is even\")\n",
    "else:\n",
    "    print(\"the provided number is odd\")\n"
   ]
